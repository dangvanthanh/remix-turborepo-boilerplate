{
	"name": "web",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"lint": "pnpm dlx @biomejs/biome lint ./app",
		"format": "pnpm dlx @biomejs/biome format ./app --write",
		"start": "remix-serve ./build/index.js",
		"typecheck": "tsc",
		"update": "pnpm dlx npm-check-updates -ui",
		"lingui:extract": "lingui extract --clean"
	},
	"dependencies": {
		"@lingui/core": "^4.11.2",
		"@lingui/detect-locale": "^4.11.2",
		"@lingui/macro": "^4.11.2",
		"@lingui/react": "^4.11.2",
		"@radix-ui/react-icons": "^1.3.0",
		"@remix-run/css-bundle": "^2.11.1",
		"@remix-run/node": "^2.11.1",
		"@remix-run/react": "^2.11.1",
		"@remix-run/serve": "^2.11.1",
		"@repo/ui": "workspace:*",
		"@unovis/react": "^1.4.3",
		"@unovis/ts": "^1.4.3",
		"accept-language-parser": "^1.5.0",
		"date-fns": "^3.6.0",
		"intl-parse-accept-language": "^1.0.0",
		"isbot": "^5.1.14",
		"lucide-react": "^0.424.0",
		"react": "^18.3.1",
		"react-day-picker": "^8.10.0",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@lingui/cli": "^4.11.2",
		"@lingui/conf": "^4.11.2",
		"@lingui/vite-plugin": "^4.11.2",
		"@remix-run/dev": "^2.11.1",
		"@repo/biome-config": "workspace:*",
		"@types/accept-language-parser": "^1.5.6",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.41",
		"tailwindcss": "^3.4.8",
		"typescript": "^5.5.4",
		"vite": "^5.4.0",
		"vite-plugin-babel-macros": "^1.0.6",
		"vite-tsconfig-paths": "^5.0.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
